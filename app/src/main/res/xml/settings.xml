<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">


    <PreferenceCategory
        android:title="@string/general">


        <SwitchPreference
            android:defaultValue="false"
            android:key="prefs_notifications"
            android:title="Enable Notifications"/>


        <Preference
            android:key="@string/send_feedback"
            android:summary="We appreciate your feedback. What did you like or dislike ?"
            android:title="Send Feedback"/>

        <Preference
            android:key="version"
            android:summary="@string/version"
            android:title="Version"/>

        <Preference
            android:key="copyright"
            android:summary="UltimateAndroidTemplateRx Â© 2017 - forever. All rights reserved."
            android:title="Copyright"/>

    </PreferenceCategory>


</PreferenceScreen>